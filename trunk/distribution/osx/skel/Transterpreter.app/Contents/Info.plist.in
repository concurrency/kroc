<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple Computer//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>CFBundleIdentifier</key>
	<string>org.transterpreter.transterpreter</string>
	<key>CFBundleName</key>
	<string>Transterpreter</string>
	<key>CFBundlePackageType</key>
	<string>APPL</string>
	<key>CFBundleVersion</key>
	<string>$version</string>
	<key>CFBundleShortVersionString</key>
	<string>$shortversion</string>
	<key>CFBundleSignature</key>
	<string>tptr</string>
	<key>SUFeedURL</key>
	<string>$sparkle_feed</string>
	<key>SUEnableSystemProfiling</key>
	<true/>
	<key>SUPublicDSAKeyFile</key>
	<string>org.transterpreter.transterpreter.pem</string>
  <key>NSAppleScriptEnabled</key>
  <string>No</string>
  <key>CFBundleAllowMixedLocalizations</key>
  <string>true</string>
	<key>LSHasLocalizedDisplayName</key>
  <true/>
  <key>CFBundleInfoDictionaryVersion</key>
  <string>6.0</string>
	<key>NSHumanReadableCopyright</key>
	<string>Copyright 2005-2010 transterpreter.org</string>
  <key>CFBundleIconFile</key>
	<string>Transterpreter.icns</string>
	<key>CFBundleExecutable</key>
  <string>JavaApplicationStub</string>
  <key>CFBundleDevelopmentRegion</key>
  <string>English</string>
  <key>Java</key>
  <dict>
    <!--
    <key>WorkingDirectory</key>
    <string>$APP_PACKAGE/Contents/Resources/</string>
    -->
    <key>Arguments</key>
		<string>-background -settings=~/.transterpreter/jEdit</string>
    <key>ClassPath</key>
    <string>$APP_PACKAGE/Contents/Resources/jEdit/jedit.jar</string>
    <key>JVMVersion</key>
    <string>1.5+</string>
    <key>MainClass</key>
    <string>org.gjt.sp.jedit.jEdit</string>
    <key>Properties</key>
    <dict>
      <key>apple.laf.useScreenMenuBar</key>
      <string>true</string>
    </dict>
	</dict>
  <key>CFBundleDocumentTypes</key>
  <array>
    <dict>
      <key>CFBundleTypeIconFile</key>
      <string>text.icns</string>
      <key>CFBundleTypeName</key>
      <string>Transterpreter bytecode</string>
      <key>CFBundleTypeExtensions</key>
      <array>
        <string>tbc</string>
      </array>
      <key>CFBundleTypeRole</key>
      <string>Viewer</string>
		</dict>
    <dict>
      <key>CFBundleTypeIconFile</key>
      <string>text.icns</string>
      <key>CFBundleTypeName</key>
      <string>occam Sourcecode</string>
      <key>CFBundleTypeExtensions</key>
      <array>
        <string>occ</string>
      </array>
      <key>CFBundleTypeRole</key>
      <string>Editor</string>
    </dict>
	</array>
</dict>
</plist>
