      VAL Dv2.in IS Dv.in[2] :
      VAL Dv1.in IS Dv.in[1] :
      VAL Dv0.in IS Dv.in[0] :
      
      PROC check.results(VAL INT Q, VAL [3]INT Dv, VAL INT Guard, VAL []BYTE msg)
        SEQ
          check(Q,     (MOSTNEG INT)+4, msg)
          check(Dv[2],   1, msg)
          check(Dv[1],   1, msg)
          check(Dv[0],   0, msg)
          check(Guard, -7, msg)
      :
      --}}}
      --{{{  variables
      INT Q, Dv2, Dv1, Dv0, Guard :
      [3]INT Dv :
