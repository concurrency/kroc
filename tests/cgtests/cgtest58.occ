          #ENDIF
          SEQ
            IF
              Yfrac[3] = Dv3.in
                Q := MaxInt
              TRUE
                Q,  Carry := LONGDIV ( Dv3.in,  Dv2.in,  Yfrac[3])
            IF
              Q <> 0                --ensure not too big
