    --{{{  access to fn result array
    PROC const.access.to.result()
      VAL n IS 3 :
      --{{{  f1
      [n]INT FUNCTION f1(VAL [n]INT y, VAL INT x)
        [n]INT res :
        #IF NOT DEFINED (COMPILER.TOCK)
        PLACE res IN WORKSPACE :
        #ENDIF
        VALOF
          SEQ
            res := y
            abbr0 IS res[0] :
            abbr0 := abbr0 + x
            abbr1 IS res[1] :
            abbr1 := abbr1 + x
            abbr2 IS res[2] :
            abbr2 := abbr2 + x
            SEQ i = 0 FOR n
              res[i] := res[i] + x
          RESULT res
      :
      --}}}
      --{{{  f2
      [n]INT FUNCTION f2(VAL [n]INT y, VAL INT x)
        [n]INT res :
        #IF NOT DEFINED (COMPILER.TOCK)
        PLACE res IN VECSPACE :
        #ENDIF
        VALOF
          SEQ
            res := y
            abbr0 IS res[0] :
            abbr0 := abbr0 + x
            abbr1 IS res[1] :
            abbr1 := abbr1 + x
            abbr2 IS res[2] :
            abbr2 := abbr2 + x
            SEQ i = 0 FOR n
              res[i] := res[i] + x
          RESULT res
      :
      --}}}
      --{{{  f3
      [n]INT FUNCTION f3(VAL [n]INT y, VAL INT x)
        [n]INT res :
        #IF NOT DEFINED (COMPILER.TOCK)
        PLACE res IN VECSPACE :
        #ENDIF
        VALOF
          SEQ
            res := y
            SEQ i = 0 FOR n
              abbri IS res[i] :
              abbri := abbri + x
            SEQ i = 0 FOR n
              res[i] := res[i] + x
          RESULT res
      :
      --}}}
    
      [n]INT x :
      SEQ
        title("access to const index into fn result")
        x := f1([9,10,11], 2)
        SEQ i = 0 FOR SIZE x
          check(x[i], 13+i, "CONST00")
        x := f2([9,10,11], 3)
        SEQ i = 0 FOR SIZE x
          check(x[i], 15+i, "CONST01")
        x := f3([9,10,11], 4)
        SEQ i = 0 FOR SIZE x
          check(x[i], 17+i, "CONST02")
    :
    const.access.to.result()
