                  W[1],  W[0] := LONGPROD ( Q,  Yfrac[0],  0)
                  W[2],  W[1] := LONGPROD ( Q,  Yfrac[1],  W[1])
                  W[3],  W[2] := LONGPROD ( Q,  Yfrac[2],  W[2])
