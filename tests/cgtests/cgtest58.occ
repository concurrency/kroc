        check.results(Q, [Dv0, Dv1, Dv2], Guard, "QReal01")
        
        Q, Dv, Guard := QRealIDiv.2(Dv.in, Guard.in, Yfrac)
        check.results(Q, Dv, Guard, "QReal02")
    
        Q, Dv, Guard := QRealIDiv.3(Dv.in, Guard.in, Yfrac)
        check.results(Q, Dv, Guard, "QReal03")
    
