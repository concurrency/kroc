                            SEQ
                              tim ? t
                              tim ? AFTER t PLUS Cancel.delay
                            #ENDIF
                            trace.sn ("commstest: ... dummy INT inputted } ...")
                        CLAIM svr
                          SEQ
                            trace.sn ("commstest: ... claimed { ...")
                        #ELSE
                        SEQ
                          SEQ
                        #ENDIF
                            svr[real32] ? x
                            #IF DEFINED (TEST.CANCEL)
                            trace.sn ("commstest: ... } ...")
                            #ENDIF
                        trace.sn ("commstest: ... done")
                        ASSERT (x = 555.666)
                      #ENDIF
                      --}}}

                      --{{{  TEST.REAL64
                      #IF DEFINED (TEST.REAL64)
                      INITIAL REAL64 x IS 999.0:
                      SEQ
                        trace.sn ("commstest: input real64...")
                        #IF DEFINED (TEST.CANCEL)
                        INT x:
